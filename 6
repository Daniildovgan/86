#include <iostream>
#include <vector>

std::vector<int> mergeArrays(int A[], int M, int B[], int N) {
    std::vector<int> result;

    for (int i = 0; i < M; ++i) {
        result.push_back(A[i]);
    }
    for (int i = 0; i < N; ++i) {
        result.push_back(B[i]);
    }

    return result;
}

int main() {
    int M, N;

    std::cout << "Введіть розмір масиву A: ";
    std::cin >> M;
    int A[M];
    std::cout << "Введіть елементи масиву A:\n";
    for (int i = 0; i < M; ++i) {
        std::cin >> A[i];
    }

    std::cout << "Введіть розмір масиву B: ";
    std::cin >> N;
    int B[N];
    std::cout << "Введіть елементи масиву B:\n";
    for (int i = 0; i < N; ++i) {
        std::cin >> B[i];
    }

    std::vector<int> result = mergeArrays(A, M, B, N);

    std::cout << "Об'єднані елементи масивів A та B:\n";
    for (int num : result) {
        std::cout << num << " ";
    }
    std::cout << std::endl;

    return 0;
}
