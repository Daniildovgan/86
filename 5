#include <iostream>
#include <unordered_set>
#include <vector>

std::vector<int> getUniqueElements(int A[], int M, int B[], int N) {
    std::unordered_set<int> setA(A, A + M); 
    std::unordered_set<int> setB(B, B + N); 
    std::vector<int> result;

   
    for (int num : setA) {
        if (setB.find(num) == setB.end()) {
            result.push_back(num);
        }
    }
    for (int num : setB) {
        if (setA.find(num) == setA.end()) {
            result.push_back(num);
        }
    }

    return result;
}

int main() {
    int M, N;

    std::cout << "Введіть розмір масиву A: ";
    std::cin >> M;
    int A[M];
    std::cout << "Введіть елементи масиву A:\n";
    for (int i = 0; i < M; ++i) {
        std::cin >> A[i];
    }

    std::cout << "Введіть розмір масиву B: ";
    std::cin >> N;
    int B[N];
    std::cout << "Введіть елементи масиву B:\n";
    for (int i = 0; i < N; ++i) {
        std::cin >> B[i];
    }

    std::vector<int> result = getUniqueElements(A, M, B, N);

    std::cout << "Унікальні елементи масивів A та B:\n";
    for (int num : result) {
        std::cout << num << " ";
    }
    std::cout << std::endl;

    return 0;
}
